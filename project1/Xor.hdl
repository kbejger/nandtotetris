/**
 * Exclusive-or gate:
 * if ((a and Not(b)) or (Not(a) and b)) out = 1, else out = 0
 */
CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a , out=n0 );
    Not(in=b , out=n1 );
    And(a=a , b=n1 , out=a0 );
    And(a=n0 , b=b , out=a1 );
    Or(a=a0 , b=a1 , out=out );      
}