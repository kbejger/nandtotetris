/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=n );
    And(a=a , b=n , out=a0 );
    And(a=b , b=sel , out=a1 );
    Or(a=a0, b=a1, out=out);  
}